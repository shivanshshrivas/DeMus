{
    "name": "demus",
    "version": "1.0.0",
    "description": "DeMus: Decentralized Music Platform",
    "scripts": {
        "start": "cd frontend && npm start",
        "build": "cd frontend && npm run build",
        "test": "cd frontend && npm test",
        "eject": "cd frontend && npm run eject",
        "lint": "cd frontend && eslint 'src/**/*.{js,jsx}'",
        "format": "cd frontend && prettier --write 'src/**/*.{js,jsx,css}'",
        "compile-contracts": "cd smart-contracts && truffle compile",
        "migrate-contracts": "cd smart-contracts && truffle migrate",
        "test-contracts": "cd smart-contracts && truffle test",
        "heroku-postbuild": "cd frontend && npm install --legacy-peer-deps && npm run build"
    },
    "keywords": [],
    "author": "Shivansh Shrivas",
    "license": "MIT",
    "devDependencies": {
        "eslint": "^7.32.0",
        "prettier": "^2.8.8",
        "truffle": "^5.11.5"
    },
    "dependencies": {
        "@helia/unixfs": "^3.0.6",
        "@openzeppelin/contracts": "^5.0.2",
        "@truffle/hdwallet-provider": "^2.1.15",
        "axios": "^1.7.2",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "form-data": "^4.0.0",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.2.0",
        "helia": "^4.2.4",
        "ipfs-core": "^0.18.1",
        "multer": "^1.4.5-lts.1",
        "uint8arrays": "^5.1.0",
        "web3": "^4.11.0"
    },
    "engines": {
        "node": ">=14.0.0",
        "npm": ">=6.0.0"
    }
}
